    $.fn.follow_author = function(){
        author_id = $(this).attr('data');
        author = $(this);
        if (gon.user && !gon.is_admin) {
            $('.universal-loader').show();
            $.post('/blog/blogs/follow_author', { id: author_id }).done(function() {
                if (author.text() == 'Follow') {
                    author.html("<p>Following</p>");
                }
                else {
                    author.html("<p>Follow</p>");
                }
                $('.universal-loader').hide();
            });
        }
        else if(gon.is_admin){
            alert('This activity is not allowed for you profile');
        }
        else{
            $('.universal-loader').show();
            $.ajax({
                type: "POST",
                url: "/blog/blogs/follow_session",
                data: {author_id: author_id}
            }).done(function () {
                $('.universal-loader').hide();
            });
        }
    };


    $.fn.follow_subcategory = function(){
        subcategory_slug = $(this).attr('data');
        subcategory = $(this);
        if (gon.user && !gon.is_admin) {
            $('.universal-loader').show();

            $.post('/blog/blogs/follow_subcategory', { subcategory_slug: subcategory_slug })
                .done(function() {
                    if (subcategory.text() == 'Follow') {
                        subcategory.html("<p>Following</p>");
                    }
                    else {
                        subcategory.html("<p>Follow</p>");
                    }
                    $('.universal-loader').hide();
                });
        }
        else if(gon.is_admin){
            alert('This activity is not allowed for you profile');
        }
        else{
            $('.universal-loader').show();
            $.ajax({
                type: "POST",
                url: "/blog/blogs/follow_session",
                data: {subcategory_slug: subcategory_slug}
            }).done(function () {
                $('.universal-loader').hide();

            });
        }
    };

</script>


